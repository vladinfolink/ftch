{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "rules": {
    "jsx-no-lambda": false,
    "no-console": false,
    "no-unused-expression": false,
    "ordered-imports": false,
    "only-arrow-functions": false,
    "member-access": false,
    "noUnusedLocals": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/3d_lib"
    ]
  }
}
